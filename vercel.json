{
  "version": 2,
  "builds": [
    {
      "src": "docs/**",
      "use": "@vercel/static"
    },
    {
      "src": "api/**/*.js",  
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/auth/github/callback",
      "dest": "/api/auth/github/callback.js"
    }
  ],
  "buildCommand": "pnpm run docs:build",  
  "outputDirectory": "docs/.vitepress/dist"  
}
